{{>layout/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">이메일</th>
        <th scope="col">비밀번호</th>
    </tr>
    </thead>
    <tbody>
    <!--반복 가능한 객체일 경우, 해당 부분을 반복-->
    {{#memberList}}
        <tr>
            <th scope="row">{{id}}</th>
            <td><a href="/member/{{id}}">{{email}}</a></td>
            <td>{{password}}</td>
        </tr>
    {{/memberList}}
    </tbody>
</table>
<a href="/member/new" class="btn btn-success">가입하기</a>
{{>layout/footer}}